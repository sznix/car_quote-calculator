# Vehicle Model Mapping System Configuration

# Data Source Configuration
# Type of data source: json, api, database
VEHICLE_DATA_SOURCE=json

# Path to JSON data file (for json data source)
VEHICLE_DATA_PATH=./assets/js/data/vehicle-models.json

# API URL for external data source (for api data source)
VEHICLE_API_URL=https://api.example.com/vehicles

# Caching Configuration
# Enable/disable caching
VEHICLE_CACHE_ENABLED=true

# Cache TTL in seconds (1 hour default)
VEHICLE_CACHE_TTL=3600

# Logging Configuration
# Enable/disable logging
VEHICLE_LOGGING_ENABLED=true

# Log level: error, warn, info, debug
VEHICLE_LOG_LEVEL=info

# Database Configuration (for future database data source)
# VEHICLE_DB_HOST=localhost
# VEHICLE_DB_PORT=5432
# VEHICLE_DB_NAME=vehicle_db
# VEHICLE_DB_USER=vehicle_user
# VEHICLE_DB_PASSWORD=vehicle_password

# API Configuration (for future enhancements)
# VEHICLE_API_RATE_LIMIT=100
# VEHICLE_API_TIMEOUT=5000
# VEHICLE_API_RETRY_ATTEMPTS=3